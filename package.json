{
  "name": "codemirror-lint-eslint",
  "version": "1.0.0",
  "description": "use ESLint to lint javascript code inside codemirror",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rm -rf .parcel-cache && rm -rf dist",
    "serve": "npm run build && http-server ./dist",
    "start": "npm run clean && parcel ./index.html",
    "build": "npm run clean && parcel build ./index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuliangsir/codemirror-lint-eslint.git"
  },
  "keywords": [
    "codemirror"
  ],
  "author": "liuliang@webfrontend.dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuliangsir/codemirror-lint-eslint/issues"
  },
  "homepage": "https://github.com/liuliangsir/codemirror-lint-eslint#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "eslint": "^6.8.0",
    "http-server": "^0.12.3",
    "parcel": "^2.0.0-alpha.3.2"
  },
  "alias": {
    "codemirror": "./lib/codemirror/lib/codemirror.js",
    "esquery": "./node_modules/esquery/dist/esquery.js"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
