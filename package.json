{
  "name": "codemirror-lint-eslint",
  "version": "1.0.0",
  "description": "use ESLint to lint javascript code inside codemirror",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .cache",
    "start": "npm run clean && parcel ./index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuliangsir/codemirror-lint-eslint.git"
  },
  "keywords": [
    "codemirror"
  ],
  "author": "liuliang@webfrontend.dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuliangsir/codemirror-lint-eslint/issues"
  },
  "homepage": "https://github.com/liuliangsir/codemirror-lint-eslint#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "eslint": "^6.8.0",
    "parcel-bundler": "^1.12.4"
  },
  "alias": {
    "codemirror": "./assets/codemirror/lib/codemirror.js"
  }
}
